<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.codeprobe.butin.repository.AccessDao">
    <insert id="insert" parameterType="cn.codeprobe.butin.model.po.Action">
        INSERT INTO access (username, ip, url, time)
        VALUES (#{username}, #{ip}, #{url},#{time})
    </insert>

</mapper>